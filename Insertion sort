public class Main
{
	public static void main(String[] args) {
	    int a[]={8,4,1,5,9,2};
	    for (int i=0;i<a.length;i++){
	        int tmp=a[i];
	        int j=i-1;
	        while(j>=0 && a[j]>tmp){
	            a[j+1]=a[j];
	            j--;
	        }
	        a[j+1]=tmp;
	    } 
	    for (int i=0;i<a.length; i++){
	        System.out.print(a[i] + " ");
	    } 
	}
}
